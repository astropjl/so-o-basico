		escreva("Insira o nome do estudante: ")
		leia (nome)
		escreva("Insira a idade do estudante: ")
		leia (idade)
		escreva("Insira a primeira nota: ")
		leia (not1)
		escreva("Insira a segunda nota: ")
		leia (not2)
